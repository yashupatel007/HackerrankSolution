vector<int> stones(int n, int a, int b) {
    vector<int>result;
   if(a==b){ // if a and b are same so the combination is same
    result.push_back((n-1)*a);
    return result;
   }
   if(a>b){ // a<b
     swap(a,b);
   }
   for(int i=0;i<n;i++){
    int res=(n-1-i)*a+i*b;
    result.push_back(res);
   }
   return result;
   
}
