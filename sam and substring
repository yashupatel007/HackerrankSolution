int substrings(string n) {
    long long a= n.size();
   long long total=0;
   long long prev=0;
   long long mod= pow(10,9)+7;
   for(long long i=1;i<=a;i++){
       int digit= n[i-1]-'0';
       long long res = (10*prev+i*digit)%mod;
       total = (total+res)%mod;
       prev=res;
   }
   return (int)total;
}
